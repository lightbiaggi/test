
@{
    ViewBag.Title = "Results";
}

@model testperoject.Models.Results

<h2>File Loading Results</h2>

<div class="row">
    <div class="col-md-6">
        <h2>Updated Clients</h2>
        <ul class="list-group">

            @foreach ( testperoject.Models.Client c in Model.UpdatedClients)
            {
                <li class="list-group-item">@c.FirstName  + @c.LastName</li>
            }
        </ul>
    </div>
    <div class="col-md-6">
        <h2>Added Clients</h2>
        <ul class="list-group">

            @foreach (testperoject.Models.Client c in Model.AddedClients)
            {
                <li class="list-group-item">@c.FirstName  + @c.LastName</li>
            }
        </ul>

    </div>
 
</div>

